{
  "name": "struct-alg",
  "version": "0.1.0",
  "author": {
    "name": "jaylenchan",
    "email": "jaylen.work@hotmail.com"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "lint": "eslint . --fix",
    "start": "nodemon"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nodemonConfig": {
    "exec": "ts-node -r tsconfig-paths/register ./ts/main.ts",
    "ignore": [
      "**/__tests__/**",
      "**/dist/**"
    ],
    "env": {
      "TS_NODE_TRANSPILE_ONLY": true,
      "TS_NODE_PROJECT": "./tsconfig.json"
    },
    "delay": 1000
  }
}
