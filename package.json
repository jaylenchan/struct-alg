{
  "name": "tsalg",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "yarn@4.2.2",
  "author": {
    "name": "jaylenchan",
    "email": "jaylen.work@hotmail.com"
  },
  "license": "MIT",
  "module": "index.ts",
  "workspaces": [
    "packages/*",
    "playground"
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "npx eslint . --fix",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.20.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "nodemon": "^3.1.3",
    "nx": "^19.3.2",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.2",
    "typescript": "^5.5.2"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged",
    "commit-msg": "bun commitlint --edit"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
